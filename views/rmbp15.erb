<!-- For highlighting regex -->
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.3.2/jquery.min.js"></script>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Цена</th>
      <th>Название</th>
      <th>Дата</th>
			<th>#</th>
			<th>Фото</th>
    </tr>
  </thead>
  <tbody>
		<% @itms.each do  |m| %>
    <tr>
			<% if m[:title].match(/[15]{2}/) %>
			<td id="price"><span class="<%= "label label-success" if m[:cost]<=100000 %>"><%= m[:cost] %></span></td>
      <td id="title"><a href="<%= m[:url] %>"><%= m[:title] %></a></td>
      <td id="date"><%= m[:date] %></td>
			<td id="owner"><span class="label label-warning"><%= m[:owner] %></span></td>
			<td id="photo_cout"><span class="<%= "label label-danger" if m[:photo_count]<=1 %>"><%= m[:photo_count] %> шт</span></td>
			<% end %>
    </tr>
		<% end %>						
  </tbody>
</table>

<script type='text/javascript'>
	$(document).ready(function(){
    regex_ssd = /[256]{3}/ig
    regex_ram = /[16]{2}/ig     
    $('table #title').highlightRegex(regex_ssd);
    $('table #title').highlightRegex(regex_ram);
  });
</script>